<p id="notice"></p>
<h1>Books</h1>
<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Body</th>
      <th colspan="3"></th>
    </tr>
  </thead>
</table>
  <h2>New book</h2>
  <%= form_with model: Book.new, local: true do |f| %>
    <div class="field">
      <label for="book_title">Title</label>
    </div>
      <%= f.text_area :title %>
    <div class="book_body">Body</div>
      <%= f.text_area :body %>
    <div class="actions">
      <%= f.submit 'Create Book' %>
    </div>
 <% end %>